            name="%Visual.name"
            viewId="org.jcryptool.visual.sigVerification.view"
            contextHelpId="org.jcryptool.visual.sigVerification.SigVerView"
            icon="platform:/plugin/org.eclipse.ui/icons/full/eview16/defaultview_misc.png">
      </Visual>
   </extension>
   <extension
         point="org.eclipse.help.toc">
       <toc
            file="$nl$/toc.xml"
            primary="false">
      </toc>
   </extension>
