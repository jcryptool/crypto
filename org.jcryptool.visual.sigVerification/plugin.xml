            class="org.jcryptool.visual.sigVerification.ui.view.SigVerView"
            icon="platform:/plugin/org.eclipse.ui/icons/full/eview16/defaultview_misc.png"
            id="org.jcryptool.visual.sigVerification.view"
            name="%view.name">
      </view>
   </extension>
  	<extension point="org.eclipse.help.contexts">
