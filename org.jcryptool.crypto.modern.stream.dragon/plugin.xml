<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.jcryptool.crypto.modern.stream.dragon.algorithm"
         point="org.jcryptool.core.operations.algorithms_cmd">
      <algorithm_cmd
            class="org.jcryptool.crypto.modern.stream.dragon.algorithm.DragonAlgorithm"
            contextHelpId="org.jcryptool.crypto.modern.stream.dragon.wizard"
            handlerClass="org.jcryptool.crypto.modern.stream.dragon.algorithm.DragonAlgorithmHandler"
            id="org.jcryptool.crypto.modern.stream.dragon.algorithm"
            name="%algorithm.name"
            type="symmetric">
      </algorithm_cmd>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="$nl$/tocUI.xml"
            primary="false">
      </toc>
   </extension>
   <extension point="org.eclipse.help.contexts">
      <contexts
            file="$nl$/contexts.xml">
      </contexts>
   </extension>

</plugin>
